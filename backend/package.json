    "@privy-io/server-auth": "^1.13.0",
    "@getvouch/sdk": "latest",
    "@airstack/node": "^0.0.7",
    "ethers": "^6.13.0",
    "fastify": "^5.2.0",
    "ioredis": "^5.4.1",
    "bull": "^4.16.3",
    "zod": "^3.24.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "node-cron": "^3.0.3",
    "axios": "^1.7.9",
