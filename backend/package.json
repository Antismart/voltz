{
  "name": "backend",
  "version": "1.0.0",
  "description": "Voltz Backend API - Fastify Server with 0G Integration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
